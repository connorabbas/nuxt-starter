{
    "name": "nuxt-ui-template-starter",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "lint": "eslint . --fix",
        "typecheck": "nuxt typecheck",
        "auth:schema": "npx @better-auth/cli generate --config server/utils/auth.ts --output server/database/schema/auth.ts -y",
        "db:generate": "drizzle-kit generate --name=init --config ./server/database/drizzle.config.ts",
        "db:migrate": "drizzle-kit migrate --config ./server/database/drizzle.config.ts",
        "db:seed": "tsx ./server/database/seeders/user-seeder.ts"
    },
    "dependencies": {
        "@iconify-json/lucide": "^1.2.68",
        "@iconify-json/simple-icons": "^1.2.53",
        "@nuxt/ui": "^4.0.0",
        "@pinia/nuxt": "^0.11.2",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue-email/html": "^0.0.3",
        "better-auth": "^1.3.27",
        "dotenv": "^17.2.3",
        "dotenv-expand": "^12.0.3",
        "drizzle-orm": "^0.44.6",
        "drizzle-seed": "^0.3.1",
        "nodemailer": "^7.0.6",
        "nuxt": "^4.1.2",
        "nuxt-csurf": "^1.6.5",
        "nuxt-email-renderer": "^2.1.2",
        "pg": "^8.16.3",
        "pinia": "^3.0.3",
        "vue-email": "^0.8.11",
        "winston": "^3.18.3",
        "zod": "^4.1.11"
    },
    "devDependencies": {
        "@nuxt/eslint": "^1.9.0",
        "@types/node": "^24.6.1",
        "@types/nodemailer": "^7.0.2",
        "@types/pg": "^8.15.5",
        "drizzle-kit": "^0.31.5",
        "eslint": "^9.36.0",
        "tsx": "^4.20.6",
        "typescript": "^5.9.2",
        "vue-tsc": "^3.1.0"
    }
}
